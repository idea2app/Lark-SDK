{
    "name": "lark-sdk",
    "version": "0.3.0",
    "license": "LGPL-3.0",
    "author": "shiy2008@gmail.com",
    "description": "Unofficial JavaScript SDK for FeiShu/Lark API",
    "keywords": [
        "feishu",
        "lark",
        "api",
        "sdk",
        "typescript"
    ],
    "source": "src/index.ts",
    "types": "dist/index.d.ts",
    "module": "dist/index.esm.js",
    "main": "dist/index.js",
    "homepage": "https://ideapp.dev/Lark-SDK/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/idea2app/Lark-SDK.git"
    },
    "bugs": {
        "url": "https://github.com/idea2app/Lark-SDK/issues"
    },
    "dependencies": {
        "@swc/helpers": "^0.3.8",
        "koajax": "^0.8.0-rc.0",
        "regenerator-runtime": "^0.13.9"
    },
    "devDependencies": {
        "@parcel/packager-ts": "2.3.2",
        "@parcel/transformer-typescript-types": "2.3.2",
        "@types/node": "^17.0.21",
        "dotenv": "^16.0.0",
        "husky": "^7.0.4",
        "jsdom": "^19.0.0",
        "lint-staged": "^12.3.6",
        "parcel": "^2.3.2",
        "prettier": "^2.6.0",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.2",
        "web-utility": "^3.6.0"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{md,json,yml,ts}": "prettier --write"
    },
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "prepare": "husky install",
        "test": "lint-staged  &&  ts-node test/index.spec.ts",
        "build": "rm -rf dist/  &&  parcel build src/index.ts",
        "prepublishOnly": "npm test  &&  npm run build"
    }
}
