{
    "name": "lark-ts-sdk",
    "version": "0.7.9",
    "license": "LGPL-3.0",
    "author": "shiy2008@gmail.com",
    "description": "Unofficial Node.js TypeScript SDK for FeiShu/Lark API",
    "keywords": [
        "feishu",
        "lark",
        "api",
        "sdk",
        "typescript"
    ],
    "source": "src/index.ts",
    "types": "dist/index.d.ts",
    "module": "dist/index.esm.js",
    "main": "dist/index.js",
    "homepage": "https://ideapp.dev/Lark-TS-SDK/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/idea2app/Lark-TS-SDK.git"
    },
    "bugs": {
        "url": "https://github.com/idea2app/Lark-TS-SDK/issues"
    },
    "dependencies": {
        "@swc/helpers": "^0.4.14",
        "koajax": "^0.8.3",
        "node-fetch": "^2.6.8",
        "regenerator-runtime": "^0.13.11",
        "tslib": "^2.4.1",
        "web-utility": "^4.0.0"
    },
    "devDependencies": {
        "@types/node": "^16.18.11",
        "@types/node-fetch": "^2.6.2",
        "dotenv": "^16.0.3",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.0",
        "prettier": "^2.8.3",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.24",
        "typedoc-plugin-mdn-links": "^2.0.2",
        "typescript": "~4.9.4"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{md,json,yml,ts}": "prettier --write"
    },
    "scripts": {
        "prepare": "husky install",
        "lint": "lint-staged",
        "debug": "ts-node test/index.spec.ts",
        "test": "npm run lint  &&  npm run debug",
        "pack-code": "rm -rf dist/  &&  tsc",
        "pack-docs": "rm -rf docs/  &&  typedoc src/",
        "build": "npm run pack-code  &&  npm run pack-docs",
        "start": "npm run pack-docs  &&  open-cli docs/index.html",
        "prepublishOnly": "npm test  &&  npm run build"
    }
}
